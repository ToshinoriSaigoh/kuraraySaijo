		<s:HGroup bottom="10" gap="10">
			<s:Button id="saveBtn" name="saveBtn" width="120" height="44" skinClass="view.utils.Skin_BtnText"/>
			<s:Button id="delBtn" name="delBtn" width="120" height="44" skinClass="view.utils.Skin_BtnText"/>
