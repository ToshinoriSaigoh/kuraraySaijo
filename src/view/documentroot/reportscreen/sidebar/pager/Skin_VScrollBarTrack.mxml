<?xml version="1.0" encoding="utf-8"?>
<s:Skin
	xmlns:fx="http://ns.adobe.com/mxml/2009"
	xmlns:s="library://ns.adobe.com/flex/spark">
	<fx:Metadata>
		[HostComponent("spark.components.Button")]
	</fx:Metadata> 
	<s:states>
		<s:State name="up" />
		<s:State name="down" />
		<s:State name="over" />
		<s:State name="disabled" />
	</s:states>
	<s:Rect left="1" right="1" top="0" bottom="0" radiusX="0" radiusY="0" alpha="0">
		<s:fill>
			<s:SolidColor color="0xffffff"/>
		</s:fill>
	</s:Rect>
	<s:Rect left="8" right="8" top="0" bottom="0" radiusX="4" radiusY="4">
		<s:fill>
			<s:LinearGradient>
				<s:GradientEntry color="0x888888" />
				<s:GradientEntry color="0x555555" />
			</s:LinearGradient>
		</s:fill>
	</s:Rect>
</s:Skin>