<?xml version="1.0" encoding="utf-8"?>
<view.utils:Skin_Btn
	xmlns:fx="http://ns.adobe.com/mxml/2009"
	xmlns:s="library://ns.adobe.com/flex/spark"
	xmlns:view.utils="view.utils.*"
	top="0"
	left="0"
	bottom="0"
	right="0"
	width="44"
	height="44">
	<view.utils:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</view.utils:states>
	<s:Rect id="bg" left="0" right="0" top="0" bottom="0" alpha="0.3" alpha.up="0.3" alpha.over="1.0" alpha.down="1.0">
		<s:fill>
			<s:SolidColor
				color="0xffffff"
				color.over="0x22eeff"
				color.down="0x00ccee"
				color.up="0xffffff"
			/>
		</s:fill>
	</s:Rect>
	<s:Group id="icon" width="100%" height="100%"/>
	<s:Label id="label" text="{hostComponent.label}" textAlign="center" verticalAlign="bottom" width="100%" height="100%"/>
</view.utils:Skin_Btn>