<?xml version="1.0" encoding="utf-8"?>
<view.utils:Skin_Btn
	xmlns:fx="http://ns.adobe.com/mxml/2009"
	xmlns:s="library://ns.adobe.com/flex/spark"
	xmlns:view.utils="view.utils.*"
	top="0"
	left="0"
	bottom="0"
	right="0"
	width="48"
	height="48">
	<view.utils:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</view.utils:states>
	<s:Ellipse left="0" right="0" top="0" bottom="0" alpha="0.8" alpha.up="0.8" alpha.over="1.0" alpha.down="1.0">
		<s:fill>
			<s:SolidColor
				color="0x000000"
				color.over="0x22eeff"
				color.up="0x000000"/>
		</s:fill>
	</s:Ellipse>
</view.utils:Skin_Btn>