<?xml version="1.0" encoding="utf-8"?>
<s:SparkSkin
	xmlns:fx="http://ns.adobe.com/mxml/2009"
	xmlns:s="library://ns.adobe.com/flex/spark"
	xmlns:view.documentroot.reportscreen.sidebar.filelistbox="view.documentroot.reportscreen.sidebar.filelistbox.*"
	alpha.disabled="0.1"
	>
	<fx:Metadata>
	<![CDATA[ 
		[HostComponent("spark.components.ButtonBar")]
	]]>
	</fx:Metadata>
	<fx:Declarations>
		<fx:Component id="middleButton">
			<s:ButtonBarButton name="{data.name}" allowDeselection="false" skinClass="view.documentroot.reportscreen.sidebar.filelistbox.Skin_OpenButtonBarBtn"/>
		</fx:Component>
	</fx:Declarations>

	<s:states>
		<s:State name="normal"/>
		<s:State name="disabled"/>
	</s:states>
	<s:DataGroup id="dataGroup" width="100%" height="100%">
		<s:layout>
			<s:HorizontalLayout gap="0"/>
		</s:layout>
	</s:DataGroup>
</s:SparkSkin>