<?xml version="1.0" encoding="utf-8"?>
<s:Skin
	xmlns:fx="http://ns.adobe.com/mxml/2009"
	xmlns:s="library://ns.adobe.com/flex/spark">
	<fx:Metadata>
		[HostComponent("spark.components.Button")]
	</fx:Metadata> 
	<s:states>
		<s:State name="up" />
		<s:State name="over" />
		<s:State name="down" />
		<s:State name="disabled" />
	</s:states>
	<s:Rect width="16" horizontalCenter="0" top="0" bottom="0" alpha="0">
		<s:fill>
			<s:SolidColor color="0xffffff"/>
		</s:fill>
	</s:Rect>
	<s:Rect width="8" horizontalCenter="0" top="0" bottom="0" radiusX="4" radiusY="4">
		<s:stroke>
			<s:SolidColorStroke color="0x333333"/>
		</s:stroke>
		<s:fill>
			<s:SolidColor color="0x222222"/>
		</s:fill>
	</s:Rect>
</s:Skin>
