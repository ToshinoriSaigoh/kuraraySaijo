<?xml version="1.0" encoding="utf-8"?>
<s:SparkSkin
	xmlns:fx="http://ns.adobe.com/mxml/2009"
	xmlns:s="library://ns.adobe.com/flex/spark"
	alpha.disabled="0.1"
	top="0"
	left="0"
	bottom="0"
	right="0"
	width="66"
	height="66"
>

	<fx:Metadata>
	<![CDATA[ 
		[HostComponent("spark.components.Button")]
	]]>
	</fx:Metadata>
	<s:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</s:states>
	<s:Rect id="fill" left="0" right="0" top="0" bottom="0" alpha="0.5" alpha.over="1.0">
		<s:fill>
			<s:SolidColor
				color="0xffffff"
			/>
		</s:fill>
	</s:Rect>
	<s:SpriteVisualElement id="body" width="66" height="66"/>
</s:SparkSkin>
