<?xml version="1.0" encoding="utf-8"?>
<view.utils:Skin_BtnCircle
	xmlns:fx="http://ns.adobe.com/mxml/2009"
	xmlns:s="library://ns.adobe.com/flex/spark"
	xmlns:controller.documentroot.reportscreen.palletcontainer.moviectrlr="controller.documentroot.reportscreen.palletcontainer.moviectrlr.*"
	xmlns:view.utils="view.utils.*"
	top="0"
	left="0"
	bottom="0"
	right="0"
	width="48"
	height="48"
	>
	<fx:Declarations>
		<controller.documentroot.reportscreen.palletcontainer.moviectrlr:MovieCtrlrBtn id="ctrlr"/>
	</fx:Declarations>
	<view.utils:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</view.utils:states>
	<s:Ellipse id="bg" left="0" right="0" top="0" bottom="0" alpha="0" alpha.up="0" alpha.over="1.0" alpha.down="1.0">
		<s:fill>
			<s:SolidColor color="0x000000"/>
		</s:fill>
	</s:Ellipse>
	<s:Group id="icon" width="100%" height="100%"/>
	<s:Label id="label" text="{hostComponent.label}" textAlign="center" verticalAlign="bottom" width="100%" height="100%"/>
</view.utils:Skin_BtnCircle>